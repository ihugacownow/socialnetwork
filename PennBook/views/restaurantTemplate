 <script>
  // Use a submit handler for the form
	$( "#searchForm" ).submit(function( event ) {
 		// Prevents form from submitting prematurely
 		event.preventDefault();
 		// rRetrieve textfield values 
 		var $form = $( this ),
   		term = $form.find( "input[name='s']" ).val(),
   		url = $form.attr( "action" );
 		// Send the data using post
 		var posting = $.post( url, { s: term } );
 		// Put the results in a div
		 posting.done(function( data ) {
			   var content = $( data ).find( "#content" );
			   $( "#result" ).empty().append( content );
			 });
		});  
  
  
  
  </script>
  
  
  
  
  window.onload = function() {
		loadMap(): 
		deleteMap();
		// process the get 
 		
		
	
		
	}
	
	var deleteMap = function() {
	 	map = null;   	
    	console.log("Refreshing the table"); 
    	 $.get('/getajaxrestaurants', function(response) 
           {
             loadMapAjax(JSON.parse(response))  
             	setInterval(deleteMap, 5 * 1000);          
             
           })
           // in case there is an error 
           .fail(function(error) { alert(error.responseJSON) }); 
            
            
    } 